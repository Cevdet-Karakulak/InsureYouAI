@model List<InsureYouAI.Entities.Category>

<section id="categories-1" class="widget widget_categories">
    <h2 class="widget-title">Kategorilerimiz</h2>
    <ul>
        @foreach (var item in Model)
        {
            <li class="cat-item cat-item-16">
                <a href="#">@item.CategoryName</a> (@(item.Articles?.Count ?? 0))
            </li>
        }
    </ul>
</section>